package ssafy.musicD;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.data.mongodb.core.MongoTemplate;

import ssafy.musicD.dto.Account;

@SpringBootApplication
public class MusicDApplication {
	
	@Autowired
	static MongoTemplate mongoTemplate;

	public static void main(String[] args) {
		SpringApplication.run(MusicDApplication.class, args);
		
		Account account = new Account();
		account.setId(2);
		account.setUsername("donghwi");
		
		Account n = mongoTemplate.insert(account);
		System.out.println(">>>" + n.getUsername());
	}

}
